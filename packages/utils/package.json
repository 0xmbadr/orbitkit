{
  "name": "@orbitkit/utils",
  "version": "0.1.5",
  "private": true,
  "description": "Common utilities for OrbitKit",
  "license": "MIT",
  "author": "OrbitKit",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./filesystem": {
      "types": "./dist/filesystem.d.ts",
      "default": "./dist/filesystem.js"
    },
    "./url": {
      "types": "./dist/url.d.ts",
      "default": "./dist/url.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --cache --max-warnings 0",
    "typecheck": "tsc --noEmit --tsBuildInfoFile .tsbuildinfo"
  },
  "dependencies": {
    "pkg-types": "^1.1.1"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.43.7",
    "@orbitkit/tsconfig": "workspace:^",
    "@types/node": "^20.12.12",
    "eslint-config-orbitkit": "workspace:^",
    "tsup": "^8.0.2"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
