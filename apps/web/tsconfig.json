{
  "extends": ["@orbitkit/tsconfig/next.json"],
  "compilerOptions": {
    "tsBuildInfoFile": ".tsbuildinfo",
    "paths": {
      "@/*": ["./src/*"],
    },
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "**/*.js",
    "**/*.cjs",
    ".eslintrc.cjs",
  ],
  "exclude": ["node_modules"],
}
